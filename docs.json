{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Mint Starter Kit",
  "colors": {
    "primary": "#c7a657",
    "light": "#154f3d",
    "dark": "#0d201a"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Documentation",
            "pages": ["docs/api-reference/introduction"]
          },
          {
            "group": "WebSocket",
            "pages": ["docs/websocket/asyncapi", "docs/websocket/intro"]
          },
          {
            "group": "gRPC",
            "pages": [
              "docs/grpc/main",
              "docs/grpc/refdata",
              "docs/grpc/orderbook",
              "docs/grpc/orders",
              "docs/grpc/trades",
              "docs/grpc/bbo",
              "docs/grpc/positions",
              "docs/grpc/balance",
              "docs/grpc/get_orders",
              "docs/grpc/cancel_order",
              "docs/grpc/modify_order"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Status",
          "href": "https://status.qfex.com",
          "icon": "signal-bars"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@qfex.com"
      }
