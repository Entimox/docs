{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Mint Starter Kit",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Documentation",
            "pages": ["docs/api-reference/introduction"]
          },
          {
            "group": "WebSocket",
            "pages": ["docs/websocket/main", "docs/websocket/asyncapi"]
          },
          {
            "group": "gRPC",
            "pages": [
              "docs/grpc/main",
              "docs/grpc/refdata",
              "docs/grpc/orderbook",
              "docs/grpc/trades",
              "docs/grpc/bbo",
              "docs/grpc/positions",
              "docs/grpc/balance",
              "docs/grpc/orders"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://docs.qfex.com/docs",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Status",
          "href": "https://status.qfex.com",
          "icon": "signal-bars"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@qfex.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Trade Now",
      "href": "https://app.qfex.com"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/qfex__",
      "github": "https://github.com/qfex-org",
      "linkedin": "https://linkedin.com/company/qfex"
    }
  }
}
