{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "QFEX API Documentation",
  "colors": {
    "primary": "#01bf81",
    "light": "#01cafe",
    "dark": "#07363e"
  },
  "background": {
    "color": {
      "light": "#f5fffc",
      "dark": "#07363e"
    }
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Documentation",
            "pages": [
              "docs/api-reference/introduction",
              "docs/api-reference/enums"
            ]
          },
          {
            "group": "WebSocket",
            "pages": ["docs/websocket/asyncapi", "docs/websocket/intro"]
          },
          {
            "group": "gRPC",
            "pages": [
              "docs/grpc/main",
              {
                "group": "Market Data",
                "pages": [
                  "docs/grpc/refdata",
                  "docs/grpc/orderbook",
                  "docs/grpc/bbo"
                ]
              },
              {
                "group": "Trade",
                "pages": [
                  "docs/grpc/orders",
                  "docs/grpc/trades",
                  "docs/grpc/positions",
                  "docs/grpc/balance",
                  "docs/grpc/get_orders",
                  "docs/grpc/cancel_order",
                  "docs/grpc/modify_order"
                ]
              }
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Status",
          "href": "https://status.qfex.com",
          "icon": "signal-bars"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@qfex.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Trade Now",
      "href": "https://app.qfex.com"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/qfex__",
      "github": "https://github.com/qfex-org",
      "linkedin": "https://linkedin.com/company/qfex"
    }
  }
}
